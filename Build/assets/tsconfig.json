{
	"compilerOptions": {
		"target": "es2020",
		"allowSyntheticDefaultImports": true,
		"incremental": true,
		"tsBuildInfoFile": "../log/tsconfig.tsbuildinfo",
		"module": "es2020",
		"moduleResolution": "node",
		"removeComments": false,
		"alwaysStrict": true,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"useDefineForClassFields": false,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"pretty": true,
		"baseUrl": "src/scripts",
		"paths": {
			"@eliashaeussler/typo3-solver/*": ["*"]
		},
		"outDir": "../../Resources/Public/JavaScript"
	}
}
